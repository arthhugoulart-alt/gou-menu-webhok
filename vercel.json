{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/webhook",
            "dest": "/api/webhook"
        },
        {
            "src": "/check/(.*)",
            "dest": "/api/check?username=$1"
        },
        {
            "src": "/list",
            "dest": "/api/list"
        },
        {
            "src": "/(.*)",
            "dest": "/api/health"
        }
    ]
}