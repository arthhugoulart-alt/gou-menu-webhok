{
    "functions": {
        "api/*.ts": {
            "runtime": "@vercel/node@3.0.0"
        }
    },
    "rewrites": [
        {
            "source": "/webhook",
            "destination": "/api/webhook"
        },
        {
            "source": "/check/:username",
            "destination": "/api/check?username=:username"
        },
        {
            "source": "/list",
            "destination": "/api/list"
        },
        {
            "source": "/",
            "destination": "/api/health"
        },
        {
            "source": "/health",
            "destination": "/api/health"
        }
    ]
}